        "# Criando uma matriz de usuários e filmes\n",
        "user_movie_matrix = df_ratings_filtered.pivot(index='userId', columns='movieId', values='rating').fillna(0)"
      ],
      "metadata": {
        "id": "I7jP89eJyaZh"
